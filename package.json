{
	"name": "@gearsnbeans/faunadb-utils",
	"version": "0.1.0",
	"description": "Utility functions for interacting with faunadb",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"main": "./dist/index.js",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [
		"nosql",
		"faunadb",
		"utilities"
	],
	"author": "Gears N' Beans",
	"license": "MIT",
	"scripts": {
		"test": "vitest --run",
		"build": "tsup && cp README.md dist/README.md",
		"build:tsc": "tsc && cp README.md dist/README.md",
		"publish": "npm run build && npm publish",
		"lint": "eslint . --ext .ts",
		"format": "prettier --write .",
		"format:docs": "npx markdown-toc -i README.md",
		"prepare": "husky install"
	},
	"dependencies": {
		"fauna": "^2.3.0",
		"faunadb": "^4.8.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^7.7.1",
		"@typescript-eslint/parser": "^7.7.1",
		"eslint": "^8.57.0",
		"husky": "^9.0.11",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.10",
		"vitest": "^1.5.2"
	}
}

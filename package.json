{
	"name": "@gearsnbeans/faunadb-utils",
	"version": "0.1.1",
	"description": "Utility functions for interacting with faunadb",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"main": "./dist/index.js",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [
		"nosql",
		"faunadb",
		"utilities"
	],
	"author": "Gears N' Beans",
	"license": "MIT",
	"scripts": {
		"test": "vitest --run",
		"build": "tsup && cp README.md dist/README.md",
		"build:tsc": "tsc && cp README.md dist/README.md",
		"publish": "npm publish",
		"lint": "eslint . --ext .ts",
		"format": "prettier --write .",
		"format:docs": "npx markdown-toc -i README.md",
		"prepare": "husky install"
	},
	"dependencies": {
		"fauna": "^2.5.0",
		"faunadb": "^4.8.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"eslint": "^9.22.0",
		"husky": "^9.1.7",
		"prettier": "^3.5.3",
		"ts-node": "^10.9.2",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2",
		"vite": "^6.2.1",
		"vitest": "^3.0.8"
	}
}
